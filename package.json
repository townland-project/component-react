{
  "name": "@townland-project/component-react",
  "version": "0.0.1",
  "description": "Townland GUI components with React for Tailwind CSS",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "keywords": [
    "townland",
    "react",
    "components",
    "gui",
    "tailwind"
  ],
  "author": {
    "name": "Townland Project",
    "email": "mail@townland.xyz",
    "url": "https://townland.xyz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/townland-project/component-react"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@storybook/addon-actions": "^6.4.15",
    "@storybook/addon-essentials": "^6.4.15",
    "@storybook/addon-links": "^6.4.15",
    "@storybook/react": "^6.4.15",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "postcss": "^8.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^3.0.17",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@townland-project/tailwind-theme": "github:townland-project/tailwind-theme"
  }
